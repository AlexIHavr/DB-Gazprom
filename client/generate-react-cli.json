{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "src/templates/default/TemplateName.component.tsx",
        "style": "src/templates/default/TemplateName.module.scss",
        "store": "src/templates/default/TemplateName.store.ts",
        "type": "src/templates/default/store.ts"
      },
      "path": "src",
      "withStyle": true,
      "withType": true,
      "withTest": false,
      "withStore": true,
      "withLazy": false,
      "withStory": false
    }
  }
}
